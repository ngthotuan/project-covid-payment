<div id="page-content">
    <!-- Invoice Header -->
    <div class="content-header">
        <div class="header-section">
            <h1>
                <i class="gi gi-usd"></i>Thanh toán hóa đơn<br><small>Hệ thống thanh toán</small>
            </h1>
        </div>
    </div>
    <!-- END Invoice Header -->

    <!-- Invoice Block -->
    <div class="block full">
        <!-- Invoice Title -->
        <div class="block-title">
            <div class="block-options pull-right">
                <a href="javascript:void(0)" class="btn btn-sm btn-alt btn-default" onclick="App.pagePrint();"><i
                            class="fa fa-print"></i> Print</a>
            </div>
            <h2><strong>Hóa đơn thanh toán dịch vụ</strong></h2>
        </div>
        <div class="text-center">
            <div class="alert alert-success alert-dismissable text-center">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                <h4><i class="fa fa-check-circle"></i> <%= msg %>. </h4>
            </div>
        </div>
    </div>
    <!-- END Invoice Block -->
</div>
